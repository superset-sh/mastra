{
  "name": "@mastra/mcp-registry-registry",
  "version": "1.0.0",
  "description": "MCP server for registry registry services.",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": "dist/stdio.js",
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsup --silent --config tsup.config.ts",
    "build:cli": "tsup src/stdio.ts --format esm --no-dts --treeshake=smallest --splitting && tsc -p tsconfig.build.json",
    "pretest": "pnpm turbo build --filter @mastra/mcp-registry-registry",
    "test": "vitest run",
    "lint": "eslint ."
  },
  "keywords": [],
  "author": "",
  "license": "Apache-2.0",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.26.0",
    "uuid": "^13.0.0",
    "zod": "^3.25.76",
    "zod-to-json-schema": "^3.24.6"
  },
  "devDependencies": {
    "@hono/node-server": "^1.19.9",
    "@internal/lint": "workspace:*",
    "@internal/types-builder": "workspace:*",
    "@mastra/core": "workspace:*",
    "@mastra/mcp": "workspace:^",
    "@types/node": "22.19.7",
    "@vitest/coverage-v8": "catalog:",
    "@vitest/ui": "catalog:",
    "@wong2/mcp-cli": "^1.13.0",
    "cross-env": "^10.1.0",
    "eslint": "^9.37.0",
    "hono": "^4.11.9",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typescript": "catalog:",
    "vitest": "catalog:"
  },
  "peerDependencies": {
    "@mastra/core": ">=1.0.0-0 <2.0.0-0"
  },
  "homepage": "https://mastra.ai",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mastra-ai/mastra.git",
    "directory": "packages/mcp-registry-registry"
  },
  "bugs": {
    "url": "https://github.com/mastra-ai/mastra/issues"
  },
  "engines": {
    "node": ">=22.13.0"
  }
}
