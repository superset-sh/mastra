{
  "name": "@internal/llm-recorder",
  "version": "0.0.1",
  "description": "LLM response recording and replay for tests",
  "type": "module",
  "private": true,
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    },
    "./vite-plugin": {
      "import": {
        "types": "./dist/vite-plugin.d.ts",
        "default": "./dist/vite-plugin.js"
      },
      "require": {
        "types": "./dist/vite-plugin.d.ts",
        "default": "./dist/vite-plugin.cjs"
      }
    }
  },
  "scripts": {
    "build": "tsup --config tsup.config.ts",
    "test": "vitest run",
    "lint": "eslint ."
  },
  "keywords": [],
  "author": "",
  "license": "Apache-2.0",
  "devDependencies": {
    "@internal/lint": "workspace:*",
    "@mastra/core": "workspace:*",
    "@types/node": "22.19.7",
    "@types/string-similarity": "^4.0.2",
    "@vitest/coverage-v8": "catalog:",
    "@vitest/ui": "catalog:",
    "eslint": "^9.36.0",
    "tsup": "^8.5.1",
    "typescript": "catalog:",
    "vitest": "catalog:"
  },
  "dependencies": {
    "diff": "^8.0.3",
    "msw": "^2.8.4",
    "string-similarity": "^4.0.4"
  },
  "homepage": "https://mastra.ai",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mastra-ai/mastra.git",
    "directory": "packages/_llm-recorder"
  },
  "bugs": {
    "url": "https://github.com/mastra-ai/mastra/issues"
  }
