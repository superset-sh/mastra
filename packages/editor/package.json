{
  "name": "@mastra/editor",
  "version": "0.6.2-alpha.0",
  "description": "Mastra Editor for agent management and instantiation",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    },
    "./composio": {
      "import": {
        "types": "./dist/composio.d.ts",
        "default": "./dist/composio.js"
      },
      "require": {
        "types": "./dist/composio.d.ts",
        "default": "./dist/composio.cjs"
      }
    },
    "./arcade": {
      "import": {
        "types": "./dist/arcade.d.ts",
        "default": "./dist/arcade.js"
      },
      "require": {
        "types": "./dist/arcade.d.ts",
        "default": "./dist/arcade.cjs"
      }
    }
  },
  "files": [
    "dist",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/composio.ts src/arcade.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts src/composio.ts src/arcade.ts --format cjs,esm --dts --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "validate:package": "echo 'Validating package.json...'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mastra-ai/mastra.git",
    "directory": "packages/editor"
  },
  "homepage": "https://mastra.ai",
  "bugs": {
    "url": "https://github.com/mastra-ai/mastra/issues"
  },
  "dependencies": {
    "@arcadeai/arcadejs": "^2.2.0",
    "@composio/core": "^0.6.3",
    "@composio/mastra": "^0.6.3",
    "@mastra/memory": "workspace:*",
    "@mastra/schema-compat": "workspace:*"
  },
  "devDependencies": {
    "@hono/node-server": "^1.14.1",
    "@internal/ai-sdk-v4": "workspace:*",
    "@internal/ai-sdk-v5": "workspace:*",
    "@internal/ai-v6": "workspace:*",
    "@mastra/core": "workspace:*",
    "@mastra/hono": "workspace:*",
    "@mastra/libsql": "workspace:*",
    "@mastra/mcp": "workspace:*",
    "hono": "^4.7.10",
    "tsup": "^8.0.2",
    "typescript": "catalog:",
    "vitest": "catalog:",
    "zod": "^3.25.76"
  },
  "peerDependencies": {
    "@mastra/core": ">=1.0.0-0 <2.0.0-0",
    "@mastra/mcp": ">=1.0.0-0 <2.0.0-0",
    "zod": "^3.25.0 || ^4.0.0"
  },
  "peerDependenciesMeta": {
    "@mastra/mcp": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=22.13.0"
  }
}
