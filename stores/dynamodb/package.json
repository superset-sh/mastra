{
  "name": "@mastra/dynamodb",
  "version": "1.0.2-alpha.0",
  "description": "DynamoDB storage adapter for Mastra",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build:lib": "tsup --silent --config tsup.config.ts",
    "prepack": "pnpx tsx ../../scripts/generate-package-docs.ts",
    "dev": "tsup --watch && tsc -p tsconfig.build.json",
    "clean": "rm -rf dist",
    "lint": "eslint .",
    "pretest": "docker compose up -d",
    "test": "vitest run",
    "posttest": "docker compose down -v",
    "pretest:watch": "docker compose up -d",
    "test:watch": "vitest watch",
    "posttest:watch": "docker compose down -v",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.902.0",
    "@aws-sdk/lib-dynamodb": "^3.902.0",
    "electrodb": "^3.5.3"
  },
  "peerDependencies": {
    "@mastra/core": ">=1.0.0-0 <2.0.0-0"
  },
  "devDependencies": {
    "@internal/lint": "workspace:*",
    "@internal/storage-test-utils": "workspace:*",
    "@internal/types-builder": "workspace:*",
    "@mastra/core": "workspace:*",
    "@types/node": "22.19.7",
    "@vitest/coverage-v8": "catalog:",
    "@vitest/ui": "catalog:",
    "eslint": "^9.37.0",
    "tsup": "^8.5.1",
    "typescript": "catalog:",
    "vitest": "catalog:"
  },
  "homepage": "https://mastra.ai",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mastra-ai/mastra.git",
    "directory": "stores/dynamodb"
  },
  "bugs": {
    "url": "https://github.com/mastra-ai/mastra/issues"
  },
  "engines": {
    "node": ">=22.13.0"
  }
}
